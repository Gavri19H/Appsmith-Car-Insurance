  "tableData": "{{\n  (() => {\n    const raw = getSnapshot.data;\n\n    // Normalize: handle array / wrapped object / JSON string\n    const rowsRaw = (() => {\n      if (Array.isArray(raw)) return raw;\n\n      if (typeof raw === \"string\") {\n        try {\n          const parsed = JSON.parse(raw);\n          if (Array.isArray(parsed)) return parsed;\n          if (Array.isArray(parsed?.data)) return parsed.data;\n          if (Array.isArray(parsed?.rows)) return parsed.rows;\n          if (Array.isArray(parsed?.result)) return parsed.result;\n          if (Array.isArray(parsed?.rt_rows)) return parsed.rt_rows;\n          if (Array.isArray(parsed?.array)) return parsed.array;\n          return [];\n        } catch (e) {\n          return [];\n        }\n      }\n\n      if (raw && typeof raw === \"object\") {\n        if (Array.isArray(raw.data)) return raw.data;\n        if (Array.isArray(raw.rows)) return raw.rows;\n        if (Array.isArray(raw.result)) return raw.result;\n        if (Array.isArray(raw.rt_rows)) return raw.rt_rows;\n        if (Array.isArray(raw.array)) return raw.array;\n      }\n\n      return [];\n    })();\n\n    // SECURITY: Filter rows based on access control\n    const rows = AccessControl.filterData(rowsRaw);\n\n    const toNum = (v) => {\n      const n = Number(v);\n      return Number.isFinite(n) ? n : 0;\n    };\n\n    // round to 2 decimals\n    const r2 = (n) => Math.round((toNum(n) + Number.EPSILON) * 100) / 100;\n\n    const sum = (key) => rows.reduce((acc, r) => acc + toNum(r?.[key]), 0);\n\n    const avg = (key) => {\n      const vals = rows\n        .map((r) => Number(r?.[key]))\n        .filter((n) => Number.isFinite(n));\n      return vals.length ? vals.reduce((a, b) => a + b, 0) / vals.length : 0;\n    };\n\n    // --- RT totals (your original requirements)\n    const total_rt_spend = sum(\"rt_spend\");\n    const total_rt_conv_value_total = sum(\"rt_conv_value_total\");\n    const total_rt_budget = sum(\"rt_budget\");\n\n    const total_rt_roas = total_rt_spend\n      ? total_rt_conv_value_total / total_rt_spend\n      : 0;\n\n    // --- y ROI: total y_revenue / total y_spend\n    const total_y_revenue = sum(\"y_revenue\");\n    const total_y_spend = sum(\"y_spend\");\n    const total_y_roi = total_y_spend ? (total_y_revenue / total_y_spend) : 0;\n\n    // --- l7 ROI: total l7_revenue / total l7_spend\n    const total_l7_revenue = sum(\"l7_revenue\");\n    const total_l7_spend = sum(\"l7_spend\");\n    const total_l7_roi = total_l7_spend ? (total_l7_revenue / total_l7_spend) : 0;\n\n    // Create a total row that keeps all existing keys (so other columns stay blank)\n    const keySet = new Set();\n    rows.forEach((r) => Object.keys(r || {}).forEach((k) => keySet.add(k)));\n\n    const totalRow = {};\n    keySet.forEach((k) => (totalRow[k] = null));\n\n    totalRow.adset_name = \"TOTAL\";\n\n    // Sums (rounded to 2 decimals)\n    totalRow.rt_spend = r2(total_rt_spend);\n    totalRow.rt_conv_value_total = r2(total_rt_conv_value_total);\n    totalRow.rt_budget = r2(total_rt_budget);\n\n    // Averages (rounded to 2 decimals)\n    totalRow.rt_cpc = r2(avg(\"rt_cpc\"));\n    totalRow.rt_cpr = r2(avg(\"rt_cpr\"));\n    totalRow.rt_cpm = r2(avg(\"rt_cpm\"));\n\n    // Ratios (1 = 100%) (rounded to 2 decimals)\n    totalRow.rt_roas = r2(total_rt_roas);\n    totalRow.y_roi = r2(total_y_roi);\n    totalRow.l7_roi = r2(total_l7_roi);\n\n    return [...rows, totalRow];\n  })()\n}}\n",
